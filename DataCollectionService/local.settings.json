{
  "IsEncrypted": false,
    "Values": {
        "AzureWebJobsStorage": "UseDevelopmentStorage=true",
        "FUNCTIONS_WORKER_RUNTIME": "dotnet-isolated",

        "Database:ConnectionString": "mongodb://localhost:27017",
        "Database:DatabaseName": "ByteFinDb",
        "Database:TemperatureCollectionName": "Temperatures",
        "Database:LocationCollectionName": "Locations",
        "Database:HumidityCollectionName": "Humidities",

        "MeasurementPrefix:Temperature": "tmp",
        "MeasurementPrefix:Humidity": "hum",
        "MeasurementPrefix:Location": "loc",

        "AllowedValues:MinTemperature": -50,
        "AllowedValues:MaxTemperature": 50,
        "AllowedValues:MinHumidity": 0,
        "AllowedValues:MaxHumidity": 100,
        "AllowedValues:MinLongitude": -180,
        "AllowedValues:MaxLongitude": 180,
        "AllowedValues:MinLatitude": -90,
        "AllowedValues:MaxLatitude": 90
    }
}